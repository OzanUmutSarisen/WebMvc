@{
}
@model List<WebMvc.Models.ViewModels.VM_Department>

<table class="table">
    <thead>
        <th>name</th>
    <th>Faculty Name</th>
        <th>Actions</th>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                <tr>
                    <td>@item.name</td>
                    <td>@item.FK_Name</td>
                    <td>
                    <a class="btn btn-danger btn-sm" href="/admin/delete?id=@item.Id">Delete</a>
                    <a class="btn btn-warning btn-sm" href="/admin/adddepartments?id=@item.Id">Update</a>
                    </td>
                </tr>
        }

    </tbody>
</table>