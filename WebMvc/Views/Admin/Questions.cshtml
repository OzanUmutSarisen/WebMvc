@{
}
@model List<WebMvc.Models.ViewModels.VM_Questions>

<table class="table">
    <thead>
    <th>quest</th>
    <th>answer</th>
    <th>quest time</th>
    <th>video Name</th>
    <th>Actions</th>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                    <tr>
                        <td>@item.quest</td>
                        <td>@item.answer</td>
                        <td>@item.questionTime</td>
                        <td>@item.FK_Name</td>
                <td>
                    <a class="btn btn-danger btn-sm" href="/admin/delete?id=@item.Id">Delete</a>
                    <a class="btn btn-warning btn-sm" href="/admin/addquestions?id=@item.Id">Update</a>
                </td>
                    </tr>
        }

    </tbody>
</table>