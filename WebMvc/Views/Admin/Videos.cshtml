@{
}
@model List<WebMvc.Models.ViewModels.VM_Video>

<table class="table">
    <thead>
    <th>name</th>
    <th>location</th>
    <th>start time</th>
    <th>couse Name</th>
    <th>Actions</th>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                <tr>
                    <td>@item.name</td>
                    <td>@item.location</td>
                    <td>@item.startTime</td>
                    <td>@item.FK_Name</td>
                    <td>
                           <a class="btn btn-danger btn-sm" href="/admin/delete?id=@item.Id">Delete</a>
                    <a class="btn btn-warning btn-sm" href="/admin/addvideos?id=@item.Id">Update</a>
                    <a class="btn btn-warning btn-sm" href="/admin/watchvideo?id=@item.Id">Watch</a>
                    </td>
                </tr>
        }

    </tbody>
</table>