@{
}
@model List<WebMvc.Models.Domain.Faculties>

<table class="table">
    <thead>
        <th>name</th>
        <th>Actions</th>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.name</td>
                <td>
                    <a class="btn btn-danger btn-sm" href="/admin/delete?id=@item.Id">Delete</a>
                    <a class="btn btn-warning btn-sm" href="/admin/addfaculties?id=@item.Id">Update</a>
                </td>
            </tr>
        }

    </tbody>
</table>